generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model cats {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  adoptionDate   DateTime? @db.Date
  birthDate      DateTime? @db.Date
  deathDate      DateTime? @db.Date
  description    String?
  lastName       String?
  name           String?
  imageUrl       String?
}
